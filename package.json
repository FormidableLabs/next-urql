  "dependencies": {
    "react-ssr-prepass": "^1.1.2"
  },
  "peerDependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-is": "^16.8.6",
    "urql": "^1.1.0"
